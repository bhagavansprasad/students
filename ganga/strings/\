#include <stdio.h>
#include <string.h>

void main()
{
	char s1[] = "independance";
	char s2[] = "dance";
	int i, j= 0;

	for(i = 0; s1[i] != '\0'; i++)
	{
		if(s1[i] == s2[j])
		{
			j++;
			printf("strstr is :%c\n", s1[i]);
		}
		
		else if(s1[i] != '\0' && s2[j] == '\0')
			printf("strstr is :%c\n", s1[i]);
	}
}
